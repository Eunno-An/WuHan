<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior"
    >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="318dp"
        android:layout_height="522dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.36"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498"
        app:srcCompat="@drawable/koreamap" />

    <TextView
        android:id="@+id/Seoul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="서울"
        android:textSize="7pt"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.177" />

    <TextView
        android:id="@+id/Gyeng_In"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경기/인천"
        android:textSize="7pt"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/Gangwon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="강원"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.121" />

    <TextView
        android:id="@+id/Gyeong_buck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경북"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.802"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.359" />

    <TextView
        android:id="@+id/Busan_Ulsan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="부산/울산"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.55" />

    <TextView
        android:id="@+id/Gyeong_Nam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경남"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.608" />

    <TextView
        android:id="@+id/Jeon_Nam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전남"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.328"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.666" />

    <TextView
        android:id="@+id/Jeon_Buck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전북"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.328"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/Chung_Nam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="충남"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.359" />

    <TextView
        android:id="@+id/Chung_Buck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="충북"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.317" />

    <TextView
        android:id="@+id/Jeju"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제주"
        android:textColor="#000000"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.904" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/gyeongInBtn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="220dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Gyeng_In" />

    <Button
        android:id="@+id/seoulBtn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="172dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Seoul" />

    <Button
        android:id="@+id/gangwon_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="84dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Gangwon" />

    <Button
        android:id="@+id/chung_nam_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="220dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Chung_Nam" />

    <Button
        android:id="@+id/chung_buck_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="132dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Chung_Buck" />

    <Button
        android:id="@+id/gyeong_buck_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Gyeong_buck" />

    <Button
        android:id="@+id/jeon_buk_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="184dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Jeon_Buck" />

    <Button
        android:id="@+id/gyeong_nam_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="96dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Gyeong_Nam" />

    <Button
        android:id="@+id/busan_ulsan_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Busan_Ulsan" />

    <Button
        android:id="@+id/jeon_nam_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginEnd="184dp"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/Jeon_Nam" />

    <Button
        android:id="@+id/jeju_Btn"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/custom_button"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintStart_toEndOf="@+id/Jeju" />

</androidx.constraintlayout.widget.ConstraintLayout>